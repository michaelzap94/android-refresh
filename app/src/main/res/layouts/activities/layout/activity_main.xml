<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mainLinearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <GridLayout
        android:id="@+id/gridLayoutConnectGame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:columnCount="3"
        android:rowCount="10">

        <Button
            android:id="@+id/button5"
            android:layout_height="50dp"
            android:layout_columnSpan="2"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentConnect3Game"
            android:text="Connect3 Game" />

        <Button
            android:id="@+id/button8"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentAnimations"
            android:text="Animations" />

        <Button
            android:id="@+id/button6"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentVideo"
            android:text="VideoPlayer" />

        <Button
            android:id="@+id/button7"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentAudio"
            android:text="AudioPlayer" />

        <Button
            android:id="@+id/playAudio"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="toggleAudio"
            android:text="Play Audio" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentYoutube"
            android:text="YoutubeAPI" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentCalculator"
            android:text="Calculator" />

        <Button
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentFrenchPhrases"
            android:text="French P" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentListViewDemo"
            android:text="1 ListView" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentEggTimer"
            android:text="Egg Timer" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentWebDownload"
            android:text="WebDownload" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentTODO"
            android:text="TODO" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentTODO"
            android:text="TODO" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentTODO"
            android:text="TODO" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentMapBasic"
            android:text="Maps" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentHikersWatch"
            android:text="Maps Hiker" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentMemorablePlaces"
            android:text="Maps Places" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentActionBar"
            android:text="SP|A.BAR|DLG" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentBasicNotes"
            android:text="BasicNotes" />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentNewsReaderBasic"
            android:text="NewsReader" />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentInstagramClone"
            android:text="InstagramC" />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="fill_horizontal"
            android:onClick="intentActionBar"
            android:text="SharedPref" />

    </GridLayout>

</FrameLayout>